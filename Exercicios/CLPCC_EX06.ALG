algoritmo "CLPCC_EX06"


var
        n1, n2 : real
        divisao : real



inicio
         escreva("Digite um numero: ")
         leia(n1)
         
         escreva("Digite outro numero: ")
         leia(n2)
         

         divisao <- n1 / n2
         escreval("Divisao = ", divisao)



fimalgoritmo
