Algoritmo "clpcc_ex16_pt2"

Var
   numero : inteiro

Inicio
      escreval("Digite um número: ")
      leia(numero)
      
      se( numero MOD 2 = 0 ) entao
         escreval(numero, " é par")
         
      senao
          escreval(numero, " é impar")

      fimse


Fimalgoritmo