Algoritmo "clpcc_ex15_mc"

Var
    k, litros : real
    c : real

Inicio
      //entrada de dados
      escreva("Digite os Km: ")
      leia(k)
      escreva("Digite os Litros: ")
      leia(litros)

      //processamento de dados
      c <- k / litros

      //saída de dados
      escreval("Valor do consumo médio é de = ", c)


Fimalgoritmo